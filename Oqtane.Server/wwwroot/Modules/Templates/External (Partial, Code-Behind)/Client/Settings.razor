@namespace [Owner].[Module]

@inherits ModuleBase

<table class="table table-borderless">
    <tr>
        <td>
            <Label For="value" HelpText="Enter a value">Name: </Label>
        </td>
        <td>
            <input id="value" type="text" class="form-control" @bind="@_value" />
        </td>
    </tr>
</table>
